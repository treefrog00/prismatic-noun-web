{
  "name": "prismatic-noun",
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "tsc && vite",
    "dev": "tsc && vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "serve": "tsc && vite preview",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.15.17",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^4.4.0",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.49",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.15",
    "tsc-files": "^1.1.4",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@sentry/react": "^9.13.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.22.1",
    "zod": "^3.25.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "tsc-files --noEmit --project tsconfig.json"
    ]
  }
}
